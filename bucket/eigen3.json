{
    "version": "3.4.0",
    "description": "Eigen is a C++ template library for linear algebra: matrices, vectors, numerical solvers, and related algorithms.",
    "homepage": "http://eigen.tuxfamily.org",
    "license": "MPL-2.0",
    "notes": "Eigen is a header-only library. No binary files are installed.",
    "architecture": {
        "64bit": {
            "url": "https://gitlab.com/libeigen/eigen/-/archive/3.4.0/eigen-3.4.0.zip",
            "hash": "SHA256:b4c198460eba6f28d34894e3a5710998818515104d6e74e5cc331ce31e46e626",
            "extract_dir": "eigen3"
        }
    },
    "bin": [],
    "env_add_path": [],
    "persist": [],
    "checkver": {
        "github": "https://gitlab.com/libeigen/eigen",
        "regex": "\"tag_name\": \"([0-9.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://gitlab.com/libeigen/eigen/-/archive/$version/eigen-$version.zip"
            }
        },
        "hash": {
            "url": "$baseurl/$matchHead.sha256",
            "regex": "([a-f\\d]{64})"
        }
    }
}
